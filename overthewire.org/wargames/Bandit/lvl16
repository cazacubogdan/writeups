# Create a temporary file for the private key
TMP_KEY=$(mktemp /tmp/id_rsa.XXXXXX)

# Write the private key to the temporary file
echo "$PRIVATE_KEY" > "$TMP_KEY"

# Set the correct permissions
chmod 600 "$TMP_KEY"

# Start the SSH agent
eval "$(ssh-agent -s)"

# Add the private key to the SSH agent
ssh-add "$TMP_KEY"

# Clean up the temporary key file after adding to the agent
rm "$TMP_KEY"

echo "SSH key added to agent and ready for use."
